{
  "name": "new-folder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "npx tsc",
    "start": "nodemon src/server.ts",
    "startjs": "nodemon dist/.",
    "jasmine": "jasmine",
    "testreset": "db-migrate down --config ./database.json --e test",
    "testup": "db-migrate --config ./database.json --e test up",
    "prettier": "prettier --config .prettierrc {,!(node_modules)/**/}*.ts --write",
    "test": "npm run build && set ENV=test && db-migrate up --env test && jasmine && db-migrate reset",
    "devup": "db-migrate up --config ./database.json --e dev",
    "devreset": "db-migrate reset --config ./database.json --e dev"
  },
